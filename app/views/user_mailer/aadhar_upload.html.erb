
<%if @user.firstname.present? || @user.lastname.present?%>
	<p>Admin with id <%=@admin_id %> uploaded aadhar of user <%=@user.firstname.capitalize rescue nil%> <%=@user.lastname.capitalize rescue nil%> </p>
<%else%>
	<p>Admin with id <%=@admin_id %> uploaded aadhar of user <%=@email%> <p>
<%end%>

<p>Email: <%=@email%></p>
<p>UserId: <%=@user.id%></p>
