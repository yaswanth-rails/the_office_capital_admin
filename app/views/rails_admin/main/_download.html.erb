<%if !ENV['S3_REGION'].nil?%>
	<%value[0]=''%>
	<% s3 = Aws::S3::Resource.new(region: ENV['S3_REGION'], access_key_id: ENV["ACCESS_KEY_ID"],secret_access_key: ENV["SECRET_ACCESS_KEY"]) %>
	<% obj = s3.bucket(ENV["KYC_BUCKET"]).object("#{value}") %>
	<% url = obj.presigned_url(:get, expires_in: 3600, response_content_disposition: "attachment; filename=#{filename}") %>
	<%= link_to "download", url, download: "#{filename}",target: "_blank",class: "btn btn-info btn-sm" %>
<%else%>
	<%= link_to "Download","#{bucket}#{value}",download: "#{filename}",class: "btn btn-info btn-sm" %>
<%end%>